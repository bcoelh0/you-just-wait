<p id="notice"><%= notice %></p>

<h1>Wait Queues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Destination</th>
      <th>Link queue</th>
      <th>Link subscribe</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wait_queues.each do |wait_queue| %>
      <tr>
        <td><%= wait_queue.name %></td>
        <td><%= wait_queue.description %></td>
        <td><%= wait_queue.destination %></td>
        <td><%= wait_queue.wait_queue_path %></td>
        <td><%= wait_queue.subscription_path %></td>
        <td><%= link_to 'Show', wait_queue %></td>
        <td><%= link_to 'Edit', edit_wait_queue_path(wait_queue) %></td>
        <td><%= link_to 'Destroy', wait_queue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wait Queue', new_wait_queue_path %>
